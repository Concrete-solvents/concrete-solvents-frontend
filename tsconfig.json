{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "module": "es6",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@Common/*": [
        "./src/common/*"
      ],
      "@Assets/*": [
        "./src/assets/*"
      ],
      "@Features/*": [
        "./src/features/*"
      ],
      "@Pages/*": [
        "./src/pages/*"
      ],
      "@Auth/*": [
        "./src/modules/auth/*"
      ],
      "@User/*": [
        "./src/modules/user/*"
      ],
      "@Chat/*": [
        "./src/modules/chat/*"
      ],
      "@Achievement/*": [
        "./src/modules/achievement/*"
      ],
      "@Group/*": [
        "./src/modules/group/*"
      ],
      "@Badge/*": [
        "./src/modules/badge/*"
      ]
    },
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
